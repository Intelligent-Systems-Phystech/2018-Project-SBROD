  annote =	"Technische Universit{\"a}t M{\"u}nchen [M{\"u}nchen] (
		 TUM )",
  bibsource =	"OAI-PMH server at api.archives-ouvertes.fr",
  description =  "International audience",
  identifier =	"hal-01505866; DOI : 10.1371/journal.pone.0170625",
  language =	"en",
  oai =  	"oai:HAL:hal-01505866v1",
  relation =	"info:eu-repo/semantics/altIdentifier/doi/10.1371/journal.pone.0170625",
  source =	"ISSN: 1932-6203; PLoS ONE; PLoS ONE, Public Library of
		 Science, 2017,
		 {$\langle$}10.1371/journal.pone.0170625{$\rangle$}",
  subject =	"Protein-protein docking; Scoring function;
		 coarse-grained potential; [ INFO.INFO-BI ] Computer
		 Science [cs]/Bioinformatics [q-bio.QM]; [ INFO.INFO-LG
		 ] Computer Science [cs]/Machine Learning [cs.LG]",
